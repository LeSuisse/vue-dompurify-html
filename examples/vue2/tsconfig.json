{
    "compilerOptions": {
        "target": "esnext",
        "module": "esnext",
        "moduleResolution": "Node",
        "isolatedModules": true,
        "skipLibCheck": true,
        "strict": true,
        "exactOptionalPropertyTypes": true,
        "paths": {
            "vue-demi": ["./node_modules/vue-demi/"]
        }
    },
    "vueCompilerOptions": {
        "target": 2.7
    },
    "include": ["src/", "vite.config.ts"]
}
