{
    "private": true,
    "name": "root-vue-dompurify-html-repo",
    "packageManager": "pnpm@10.12.1",
    "scripts": {
        "typecheck": "turbo run typecheck",
        "build": "turbo run build",
        "test": "turbo run test",
        "test-mutation": "turbo run test-mutation",
        "lint": "eslint --flag unstable_native_nodejs_ts_config '**/*.ts' '**/*.mts' && prettier --check '**/*.md' '**.json' './.github/'",
        "lint-fix": "eslint --flag unstable_native_nodejs_ts_config '**/*.ts' '**/*.mts' --fix && prettier --write '**/*.md' '**/*.json' './.github/'"
    },
    "devDependencies": {
        "@eslint/js": "9.29.0",
        "eslint": "9.29.0",
        "eslint-config-prettier": "10.1.5",
        "eslint-plugin-prettier": "5.4.1",
        "prettier": "3.5.3",
        "turbo": "2.5.4",
        "typescript": "5.8.3",
        "typescript-eslint": "8.34.1"
    }
}
